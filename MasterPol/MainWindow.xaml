<Window x:Class="MasterPol.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MasterPol"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        
            <Label Background="#F4E8D3" Height="50"></Label>
        <StackPanel Grid.Row="0">
            <Image Height="40" Width="40" Source="/Res/Мастер пол.png"  HorizontalAlignment="Left" Margin="10,5" VerticalAlignment="Center"/>
        </StackPanel>

        <StackPanel Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Center">
            <TextBlock Text="«Мастер пол»" FontFamily="Segoe UI" FontSize="21"></TextBlock>
        </StackPanel>

        <Frame x:Name="NewFrame" NavigationUIVisibility="Hidden"></Frame>
        
    </Grid>
</Window>
